{
  "name": "alpen_safe",
  "version": "1.0.0",
  "description": "Alpen Safe - Transformed from Deno to TypeScript",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only main.ts",
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/main.js",
    "check": "tsc --noEmit",
    "fmt": "prettier --write \"**/*.ts\" --ignore-path .gitignore",
    "seed": "ts-node ./scripts/genSeed.ts",
    "test": "mocha --require ts-node/register 'test/**/*.ts'",
    "lint": "eslint . --fix",
    "serve": "ts-node main.ts",
    "serve:prod": "npm run build && node ./dist/main.js",
    "seed:psbts": "ts-node supabase/seeders/update_psbts.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "axios": "^1.8.4",
    "bip32": "^5.0.0-rc.0",
    "bitcoinjs-lib": "^6.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ecpair": "^3.0.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "tiny-secp256k1": "^2.2.3",
    "ts-case-convert": "^2.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.20",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.16",
    "chai": "^5.2.0",
    "mocha": "^11.1.0",
    "prettier": "^3.2.5",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
